{
    "compilerOptions": {
    "module": "commonjs",
    "target": "es2015",
    "declaration": true,
    "outDir": ".",
    "noResolve": false,
      "experimentalDecorators": true,
      "sourceMap": false,
      "skipLibCheck": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "resolveJsonModule": true,
      "esModuleInterop": true
    },
    "include": [
      "./*.mjs",
      "rules/*.ts"
    ],
    "exclude": []
  }