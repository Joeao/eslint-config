{
	"name": "@joeao/eslint-config-joe",
	"version": "6.2.1",
	"description": "This is a standard code style for Javascript, based on eslint-config-airbnb-base with some tweaks.",
	"main": "index.js",
	"scripts": {
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"lintfix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
	},
	"author": "",
	"license": "ISC",
	"private": false,
	"dependencies": {
		"@antriver/eslint-config-antriver": "^2.0.0",
		"@typescript-eslint/eslint-plugin": "^7.7.0",
		"@typescript-eslint/parser": "^7.7.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"eslint": "8.56.0",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-plugin-import": "2.29.1",
		"eslint-plugin-jsdoc": "46.9.1"
	},
	"overrides": {
		"@antriver/eslint-config-antriver": {
			"eslint": "$eslint",
			"eslint-config-airbnb-base": "$eslint-config-airbnb-base",
			"eslint-plugin-import": "$eslint-plugin-import",
			"eslint-plugin-jsdoc": "$eslint-plugin-jsdoc"
		}
	}
}