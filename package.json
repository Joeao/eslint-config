{
	"name": "@joeao/eslint-config-joe",
	"version": "7.0.0",
	"description": "This is a standard code style for Javascript, based on eslint-config-airbnb-base with some tweaks.",
	"main": "index.js",
	"scripts": {
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"lintfix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
	},
	"author": "",
	"license": "ISC",
	"private": false,
	"dependencies": {
		"@antriver/eslint-config-antriver": "^2.0.0",
		"@typescript-eslint/eslint-plugin": "8.6.0",
		"@typescript-eslint/parser": "^8.6.0",
		"eslint-plugin-react": "^7.36.1",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-react-refresh": "^0.4.12",
		"eslint": "9.10.0",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-plugin-import": "2.30.0",
		"eslint-plugin-jsdoc": "50.2.4"
	},
	"overrides": {
		"@antriver/eslint-config-antriver": {
			"eslint": "$eslint",
			"eslint-config-airbnb-base": "$eslint-config-airbnb-base",
			"eslint-plugin-import": "$eslint-plugin-import",
			"eslint-plugin-jsdoc": "$eslint-plugin-jsdoc"
		}
	}
}